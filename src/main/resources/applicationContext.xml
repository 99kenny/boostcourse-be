<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="userBean" class="kr.co.example.diexam01.UserBean"></bean>
	<!-- kr.or.connect.diexam01.UserBean userBean = new kr.or.connect.diexam01.UserBean(); 으로 객체 생성하게 됨 -->
	<bean id="e" class="kr.co.example.diexam01.Engine"/>
	<bean id="c" class="kr.co.example.diexam01.Car">
	<!--c객체의 property(setter, getter) 중 engine이 들어간 property(setEngine)에 e객체를 주입해라! -->
		<property name="engine" ref="e"></property>
	</bean>
</beans> 